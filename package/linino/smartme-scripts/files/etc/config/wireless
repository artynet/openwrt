config wifi-device 'radio0'
	option type 'mac80211'
	option disabled '0'
	option channel '11'
	option hwmode '11g'
	option htmode 'HT20'
 	# option path 'platform/bcm2708_usb/usb1/1-1/1-1.4/1-1.4:1.0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'SmartME'
	option encryption 'none'
